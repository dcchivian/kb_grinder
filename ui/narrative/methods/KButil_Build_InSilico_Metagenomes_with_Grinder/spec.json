{
    "ver": "0.0.1",
    "authors": [
        "dylan"
    ],
    "contact": "help@kbase.us",
    "visible": true,
    "categories": ["active","util","reads","communities"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [ 
        {
            "id": "desc",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
		"is_output_name": false
            }
        },
        {
            "id": "input_refs",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFile.PairedEndLibrary" ]
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseSets.ReadsSet" ],
		"is_output_name": true
            }
        },
        {
            "id": "reads_num",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "10000" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int"
            }
        },
        {
            "id": "population_percs",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "Genome\tSmpl_1\tSmpl_2\tSmpl_3\tetc.\ngenome-1\t10.0%\t10.0%\t10.0%\ngenome-2\t10.0%\t10.0%\t10.0%\ngenome-3\t10.0%\t10.0%\t10.0%\ngenome-4\t10.0%\t10.0%\t10.0%\ngenome-5\t10.0%\t10.0%\t10.0%\ngenome-6\t10.0%\t10.0%\t10.0%\ngenome-7\t10.0%\t10.0%\t10.0%\ngenome-8\t10.0%\t10.0%\t10.0%\ngenome-9\t10.0%\t10.0%\t10.0%\ngenome-10\t10.0%\t10.0%\t10.0%\n" ],
            "field_type": "textarea",
            "textarea_options": {
		"n_rows": "11"
            }
        },
        {
            "id": "seed",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int"
            }
        }
    ],

    "parameter-groups": [
     	{
	    "id": "subsample_fraction",
            "parameters": ["reads_num", "population_percs"],
            "optional": false,
            "allow_multiple": false,
            "with_border": true
        }
    ],

    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_util_dylan",
            "method": "KButil_Build_InSilico_Metagenomes_with_Grinder",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "desc",
                    "target_property": "desc"
                },
                {
                    "input_parameter": "input_refs",
                    "target_property": "input_refs",
		    "target_type_transform": "list<resolved-ref>"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },
                {
                    "input_parameter": "seed",
                    "target_property": "seed"
                },
                {
                    "input_parameter": "subsample_fraction",
                    "target_property": "subsample_fraction"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
		    "constant_value": "5",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}